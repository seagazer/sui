<?xml version="1.0" encoding="utf-8"?>
<com.seagazer.ui.widget.BackgroundView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/background_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BackgroundActivity">

    <com.seagazer.ui.widget.AnimLogoView
        android:id="@+id/logo_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:onClick="onClick"
        android:padding="56dp"
        app:autoPlay="true"
        app:gradientColor="@color/pink_700"
        app:logoName="安奥港哈尔" />

</com.seagazer.ui.widget.BackgroundView>